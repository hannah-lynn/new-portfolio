rt quokka from '../../assets/images/quokka.png';\nimport half from '../../assets/images/half-circles.png';\nimport halfLg from '../../assets/images/lg-half-circ.png';\n\nconst CONSTANTS = {\n  DELETING_SPEED: 30,\n  TYPING_SPEED: 150,\n};\n\nfunction Hero({ messages }) {\n  const [state, setState] = useState({\n    text: '',\n    message: '',\n    isDeleting: false,\n    loopNum: 0,\n    typingSpeed: CONSTANTS.TYPING_SPEED,\n  });\n\n  useEffect(() => {\n    let timer = '';\n    const handleType = () => {\n      setState((cs) => ({\n        ...cs, // cs means currentState\n        text: getCurrentText(cs),\n        typingSpeed: getTypingSpeed(cs),\n      }));\n      timer = setTimeout(handleType, state.typingSpeed);\n    };\n    handleType();\n    return () => clearTimeout(timer);\n  }, [state.isDeleting]);\n\n  useEffect(() => {\n    if (!state.isDeleting && state.text === state.message) {\n      setTimeout(() => {\n        setState((cs) => ({\n          ...cs,\n          isDeleting: true,\n        }));\n      }, 500);\n    } else if (state.isDeleting && state.text === '') {\n      setState((cs) => ({\n        ...cs, // cs means currentState\n        isDeleting: false,\n        loopNum: cs.loopNum + 1,\n        message: getMessage(cs, messages),\n      }));\n    }\n  }, [state.text, state.message, state.isDeleting, messages]);\n\n  function getCurrentText(currentState) {\n    return currentState.isDeleting\n      ? currentState.message.substring(0, currentState.text.length - 1)\n      : currentState.message.substring(0, currentState.text.length + 1);\n  }\n\n  function getMessage(currentState, data) {\n    return data[Number(currentState.loopNum) % Number(data.length)];\n  }\n\n  function getTypingSpeed(currentState) {\n    return currentState.isDeleting\n      ? CONSTANTS.TYPING_SPEED\n      : CONSTANTS.DELETING_SPEED;\n  }\n\n  return (\n    <div className='hero' id='home'>\n      <div className='half-container'>\n        <img\n          src={halfLg}\n          alt='green half circle with red dot'\n          className='red-dot'\n        />\n      </div>\n      <div\n        className='change m-auto mt-20 main flex items-center justify-around rounded-lg'\n        id='main'\n      >\n        <div className='main-text flex w-full justify-center space-x-2 sm:space-x-6 md:space-x-24'>\n          <div className='cta-info flex flex-col justify-center '>\n            <h2 className='text-2xl md:text-3xl flex flex-col md:items-start'>\n              Hello,\n              <div className='message'>\n                <span>{state.text}</span>\n                <span id='cursor' />\n              </div>\n            </h2>\n            <div className='items-center mr-4 mt-6'>\n              <a\n                className='contact-btn py-2 px-4 items-center rounded-lg cursor-pointer'\n                href='/new-portfolio/#contact'\n              >\n                Contact\n              </a>\n            </div>\n          </div>\n\n          <img src={quokka} alt='A quokka and I' className='quokka' />\n        </div>\n      </div>\n      <div className='half-container'>\n        <img src={half} alt='green half circles' className='half' />\n      </div>\n    </div>\n  );\n}\n\nexport default Hero;\n","/Users/hannahlynn/Desktop/Portfolio Site/portfolio-site/src/components/about/about.js",[],"/Users/hannahlynn/Desktop/Portfolio Site/portfolio-site/src/components/projects/projects.js",[],"/Users/hannahlynn/Desktop/Portfolio Site/portfolio-site/src/components/footer/footer.js",[],"/Users/hannahlynn/Desktop/Portfolio Site/portfolio-site/src/components/contact/contact.js",[],"/Users/hannahlynn/Desktop/Portfolio Site/portfolio-site/src/components/darkmode/darkmode.js",[],"/Users/hannahlynn/Desktop/Portfolio Site/portfolio-site/src/utils/themes.js",[],{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":33,"column":6,"nodeType":"57","endLine":33,"endColumn":24,"suggestions":"58"},"no-nat